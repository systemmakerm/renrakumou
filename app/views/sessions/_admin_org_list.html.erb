<table>
  <tr>
    <th>団体コード</th>
    <th>団体名</th>
  </tr>
  <% @accounts.each do |account| %>
    <tr>
      <td>
        <%= account.administrator.organization_id %>
      </td>
      <td>
        <%= link_to account.administrator.organization.name, select_org_sessions_path(:account_id => account.id) %>
      </td>
    </tr>
  <% end %>
</table>
